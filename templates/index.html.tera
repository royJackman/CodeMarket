{% extends "base" %}

{% block content %}
    <h1>The Rust Market</h1>
    <h2>Here we sell Rust primitives for bits</h2>
    <h3>Available Vendors:</h3>
    <ul>
        {% for v in market.vendors -%}
            <li><a class="button" href="/vendors/{{ v.url }}">{{v.name}}</a></li>
        {% endfor -%}
    </ul>
{% endblock content %}