{% extends "base" %}

{% block content %}
    <h1>The Rust Market</h1>
    <p>Here we sell Rust primitives for bits</p>
    <br/>
    <h3>Available Vendors:</h3>
    <ul>
        {% for v in market.vendors -%}
            <li><a href="/vendors/{{ v.url }}">{{v.name}}</a></li>
        {% endfor -%}
    </ul>
{% endblock content %}